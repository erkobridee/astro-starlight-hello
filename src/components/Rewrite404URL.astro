<script>
  console.log('astro component to do something at the 404 page');

  // https://www.30secondsofcode.org/js/s/modify-url-without-reload/
  if (window.history && window.history.state) {
    const { prevUrl } = window.history.state;
    if (prevUrl) {
      window.history.replaceState({}, '', prevUrl);
    }
  }
</script>
